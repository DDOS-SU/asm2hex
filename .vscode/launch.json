    "version": "0.2.0",
    "configurations": [
        {
            "name": "ASM2HEX Debug",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "windows": {
                "env": {
                    "CGO_CFLAGS": "-ID:/works/asm2hex/bindings/include -O3 -g -Wall -Werror",
                    "CGO_LDFLAGS": "-LD:/works/asm2hex/bindings/lib -lcapstone -lkeystone -O3 -g"
                }
            },
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}"
        }
    ]
}